    <amx:deck id="deck1" displayedChild="#{pageFlowScope.summaryView}" inlineStyle="width:100%;height:100%">
        <amx:transition triggerType="backNavigate" id="t1" transition="slideEnd"/>
        <amx:transition triggerType="forwardNavigate" id="t2" transition="slideStart"/>
        <amx:filmStrip id="summary" orientation="horizontal"
                       pageControlPosition="#{deviceScope.hardware.screen.diagonalSize gt 6 ? 'none' : 'bottom'}"
                       halign="center" valign="middle"
                       inlineStyle="width:#{deviceScope.hardware.screen.availableWidth}px" itemsPerPage="1"
                       styleClass="amx-filmStrip-stretchItems">
            <amx:filmStripItem id="summaryp1" rendered="#{deviceScope.hardware.screen.diagonalSize le 6}">
                <amx:listView id="lv1" styleClass="listViewNoSelect">
                    <amx:listItem showLinkIcon="false" id="spli5">
                        <amx:tableLayout width="100%" id="sptl5">
                            <amx:rowLayout id="sprl5">
                                <amx:cellFormat width="40px" halign="center" id="spcf51">
                                    <amx:image source="/images/func_mobilePhone_32@2x.png" id="spi5"
                                               styleClass="profilesummary_image"/>
                                </amx:cellFormat>
                                <amx:cellFormat width="100%" height="43px" id="spcf52">
                                    <amx:commandLink text="#{Mobile.inputValue}" id="cl3"
                                                     styleClass="adfmf-commandLink-noUnderline"
                                                     actionListener="#{UtilBean.sendSMS}">
                                        <amx:setPropertyListener id="spl3" from="#{Mobile.inputValue}"
                                                                 to="#{applicationScope.sms}"/>
                                    </amx:commandLink>
                                </amx:cellFormat>
                            </amx:rowLayout>
                        </amx:tableLayout>
                    </amx:listItem>
                    <amx:listItem showLinkIcon="false" id="spli4">
                        <amx:tableLayout width="100%" id="sptl4">
                            <amx:rowLayout id="sprl4">
                                <amx:cellFormat width="40px" halign="center" id="spcf41">
                                    <amx:image source="/images/func_phone_32@2x.png" id="spi4"
                                               styleClass="profilesummary_image"/>
                                </amx:cellFormat>
                                <amx:cellFormat width="100%" height="43px" id="spcf42">
                                    <amx:goLink text="#{Phone.inputValue}" id="gl6" url="facetime:#{Phone.inputValue}"
                                                styleClass="adfmf-goLink-noUnderline"
                                                rendered="#{deviceScope.device.os eq 'iOS'}"/>
                                    <amx:commandLink text="#{Phone.inputValue}" id="cl2"
                                                     styleClass="adfmf-commandLink-noUnderline"
                                                     actionListener="#{UtilBean.sendSMS}"
                                                     rendered="#{deviceScope.device.os ne 'iOS'}">
                                        <amx:setPropertyListener id="spl2" from="#{Phone.inputValue}"
                                                                 to="#{applicationScope.sms}"/>
                                    </amx:commandLink>
                                </amx:cellFormat>
                            </amx:rowLayout>
                        </amx:tableLayout>
                    </amx:listItem>
                    <amx:listItem showLinkIcon="false" id="spli3">
                        <amx:tableLayout width="100%" id="sptl3">
                            <amx:rowLayout id="sprl3">
                                <amx:cellFormat width="40px" halign="center" id="spcf31">
                                    <amx:image source="/images/func_email_32@2x.png" id="spi3"
                                               styleClass="profilesummary_image"/>
                                </amx:cellFormat>
                                <amx:cellFormat width="100%" height="43px" id="spcf32">
                                    <amx:commandLink text="#{Email.inputValue}" id="gl1"
                                                     styleClass="adfmf-commandLink-noUnderline"
                                                     actionListener="#{UtilBean.sendEmail}">
                                        <amx:setPropertyListener id="spl1" from="#{Email.inputValue}"
                                                                 to="#{applicationScope.email}"/>
                                    </amx:commandLink>
                                </amx:cellFormat>
                            </amx:rowLayout>
                        </amx:tableLayout>
                    </amx:listItem>
                    <amx:listItem showLinkIcon="false" id="spli6">
                        <amx:tableLayout width="100%" id="sptl6">
                            <amx:rowLayout id="sprl6">
                                <amx:cellFormat width="40px" halign="center" id="spcf61">
                                    <amx:image source="/images/func_twitter_32@2x.png" id="spi6"
                                               styleClass="profilesummary_image"/>
                                </amx:cellFormat>
                                <amx:cellFormat width="100%" height="43px" id="spcf62">
                                    <amx:goLink text="#{Twitter.inputValue}" id="gl4"
                                                url="twitter://user?screen_name=#{Twitter.inputValue}"
                                                styleClass="adfmf-goLink-noUnderline"/>
                                </amx:cellFormat>
                            </amx:rowLayout>
                        </amx:tableLayout>
                    </amx:listItem>
                    <amx:listItem showLinkIcon="false" id="spli7">
                        <amx:tableLayout width="100%" id="sptl7">
                            <amx:rowLayout id="sprl7">
                                <amx:cellFormat width="40px" halign="center" id="spcf71">
                                    <amx:image source="/images/func_facebook_32@2x.png" id="spi7"
                                               styleClass="profilesummary_image"/>
                                </amx:cellFormat>
                                <amx:cellFormat width="100%" height="43px" id="spcf72">
                                    <amx:goLink text="#{Facebook.inputValue}" id="gl2"
                                                url="fb://mailbox/compose/#{Facebook.inputValue}"
                                                styleClass="adfmf-goLink-noUnderline"/>
                                </amx:cellFormat>
                            </amx:rowLayout>
                        </amx:tableLayout>
                    </amx:listItem>
                    <amx:listItem showLinkIcon="false" id="spli8">
                        <amx:tableLayout width="100%" id="sptl8">
                            <amx:rowLayout id="sprl8">
                                <amx:cellFormat width="40px" halign="center" id="spcf81">
                                    <amx:image source="/images/func_linkedin_32_2x.png" id="spi8"
                                               styleClass="profilesummary_image"/>
                                </amx:cellFormat>
                                <amx:cellFormat width="100%" height="43px" id="spcf82">
                                    <amx:goLink text="#{LinkedIn.inputValue}" id="gl3"
                                                url="linkedin://profile/#{LinkedIn.inputValue}"
                                                styleClass="adfmf-goLink-noUnderline"/>
                                </amx:cellFormat>
                            </amx:rowLayout>
                        </amx:tableLayout>
                    </amx:listItem>
                    <amx:listItem showLinkIcon="false" id="spli2">
                        <amx:tableLayout width="100%" id="sptl2">
                            <amx:rowLayout id="sprl2">
                                <amx:cellFormat width="40px" halign="center" id="spcf21">
                                    <amx:image source="/images/func_gps_32@2x.png" id="spi2"
                                               styleClass="profilesummary_image"/>
                                </amx:cellFormat>
                                <amx:cellFormat width="100%" height="43px" id="spcf22">
                                    <amx:outputText value="#{City.inputValue}, #{State.inputValue}" id="cl1"/>
                                </amx:cellFormat>
                            </amx:rowLayout>
                        </amx:tableLayout>
                    </amx:listItem>
                    <amx:listItem showLinkIcon="false" id="spli1" rendered="#{MgrId.inputValue gt 0}">
                        <amx:tableLayout width="100%" id="sptl1">
                            <amx:rowLayout id="sprl1">
                                <amx:cellFormat width="40px" halign="center" id="spcf11">
                                    <amx:image source="pics/#{MgrImage.inputValue}"
                                               id="i5"
                                               inlineStyle="width:25px;height:25px;-webkit-border-radius:25px;border:1px #ccc solid;margin-top:4px"/>
                                </amx:cellFormat>
                                <amx:cellFormat height="43px" id="cf34" width="100%">
                                    <amx:panelGroupLayout id="pgl3" layout="horizontal" valign="bottom">
                                        <amx:commandLink text="#{MgrFirstName.inputValue} #{MgrLastName.inputValue}"
                                                         id="ot13"
                                                         actionListener="#{applicationScope.UtilBean.gotoPersonDetail}"
                                                         styleClass="adfmf-commandLink-noUnderline">
                                            <amx:setPropertyListener id="spl4" from="#{MgrId.inputValue}"
                                                                     to="#{applicationScope.UtilBean.id}"/>
                                        </amx:commandLink>
                                        <amx:spacer id="s3" width="10"/>
                                        <amx:outputText value="(Manager)" id="ot57" inlineStyle="color:#595960"/>
                                    </amx:panelGroupLayout>
                                </amx:cellFormat>
                            </amx:rowLayout>
                        </amx:tableLayout>
                    </amx:listItem>
                </amx:listView>
            </amx:filmStripItem>
            <amx:filmStripItem id="summaryp2" rendered="#{deviceScope.hardware.screen.diagonalSize le 6}">
                <amx:panelStretchLayout id="psl5" scrollPolicy="scroll">
                    <amx:facet name="center">
                        <amx:tableLayout id="sprating" width="100%">
                            <amx:rowLayout id="perfhdrs">
                                <amx:cellFormat id="cfrating" width="50%" halign="center">
                                    <amx:outputText value="RATING" id="otrating" inlineStyle="font-weight:lighter"/>
                                </amx:cellFormat>
                                <amx:cellFormat id="cfpotential" width="50%" halign="center">
                                    <amx:outputText value="POTENTIAL" id="otpotential"
                                                    inlineStyle="font-weight:lighter"/>
                                </amx:cellFormat>
                            </amx:rowLayout>
                            <amx:rowLayout id="perfleds">
                                <amx:cellFormat id="cfratingled" width="50%" halign="center">
                                    <dvtm:ledGauge id="ledrating" value="#{Rating.inputValue}"
                                                   inlineStyle="padding:5px;height:60px;width:60px">
                                        <dvtm:threshold id="tl11" maxValue="2" color="#ED6647"/>
                                        <dvtm:threshold id="tl12" maxValue="3" color="#FAD55C"/>
                                        <dvtm:threshold id="tl14" maxValue="5" color="#68C182"/>
                                    </dvtm:ledGauge>
                                </amx:cellFormat>
                                <amx:cellFormat id="cfpotentialled" width="50%" halign="center">
                                    <dvtm:ledGauge id="ledpotential" value="#{Potential.inputValue}"
                                                   inlineStyle="padding:5px;height:60px;width:60px">
                                        <dvtm:threshold id="t2t1" maxValue="2" color="#ED6647"/>
                                        <dvtm:threshold id="t2t2" maxValue="3" color="#FAD55C"/>
                                        <dvtm:threshold id="t2t4" maxValue="5" color="#68C182"/>
                                    </dvtm:ledGauge>
                                </amx:cellFormat>
                            </amx:rowLayout>
                            <amx:rowLayout id="perfftrs">
                                <amx:cellFormat id="cfratingftr" width="50%" halign="center">
                                    <amx:outputText value="Below" id="otratingftr1" rendered="#{Rating.inputValue lt 3}"
                                                    styleClass="profilesummary_subtext"/>
                                    <amx:outputText value="Average" id="otratingftr2"
                                                    rendered="#{Rating.inputValue eq 3}"
                                                    styleClass="profilesummary_subtext"/>
                                    <amx:outputText value="Outstanding" id="otratingftr3"
                                                    rendered="#{Rating.inputValue eq 4}"
                                                    styleClass="profilesummary_subtext"/>
                                    <amx:outputText value="Exceptional" id="otratingftr4"
                                                    rendered="#{Rating.inputValue eq 5}"
                                                    styleClass="profilesummary_subtext"/>
                                </amx:cellFormat>
                                <amx:cellFormat id="cfpotentialftr" width="50%" halign="center">
                                    <amx:outputText value="Below" id="otpotenftr1"
                                                    rendered="#{Potential.inputValue lt 3}"
                                                    styleClass="profilesummary_subtext"/>
                                    <amx:outputText value="Average" id="otpotenftr2"
                                                    rendered="#{Potential.inputValue eq 3}"
                                                    styleClass="profilesummary_subtext"/>
                                    <amx:outputText value="Outstanding" id="otpotenftr3"
                                                    rendered="#{Potential.inputValue eq 4}"
                                                    styleClass="profilesummary_subtext"/>
                                    <amx:outputText value="Exceptional" id="otpotenftr4"
                                                    rendered="#{Potential.inputValue eq 5}"
                                                    styleClass="profilesummary_subtext"/>
                                </amx:cellFormat>
                            </amx:rowLayout>
                            <amx:rowLayout id="ro2">
                                <amx:cellFormat id="cftenurehdr" width="50%" halign="center" height="40"
                                                valign="bottom">
                                    <amx:outputText value="TENURE" id="ottenurehdr1" inlineStyle="font-weight:lighter"/>
                                </amx:cellFormat>
                                <amx:cellFormat id="cfcomphdr" width="50%" halign="center" height="40" valign="bottom">
                                    <amx:outputText value="COMP RATIO" id="cfcomphdr1"
                                                    inlineStyle="font-weight:lighter"/>
                                </amx:cellFormat>
                            </amx:rowLayout>
                            <amx:rowLayout id="compleds">
                                <amx:cellFormat id="cftenled" width="50%" halign="center">
                                    <dvtm:ledGauge id="ledGauge1" value="#{Tenure.inputValue}"
                                                   inlineStyle="padding:5px;height:60px;width:60px">
                                        <dvtm:threshold id="l1tent1" maxValue="4" color="#FAD55C"/>
                                        <dvtm:threshold id="l1tent2" maxValue="6" color="#0572ce"/>
                                        <dvtm:threshold id="l1tent3" maxValue="10" color="#68C182"/>
                                    </dvtm:ledGauge>
                                </amx:cellFormat>
                                <amx:cellFormat id="cfcompled" width="50%" halign="center">
                                    <dvtm:statusMeterGauge id="statusMeterGauge1" orientation="circular"
                                                           value="#{CompRatio.inputValue}"
                                                           inlineStyle="padding:5px;height:60px;width:60px"
                                                           minValue="80" maxValue="120" plotArea="on"
                                                           thresholdDisplay="currentOnly" indicatorSize=".2"
                                                           color="Black" animationOnDataChange="auto"
                                                           animationDuration="300" animationOnDisplay="auto">
                                        <dvtm:threshold id="tthcomp1" maxValue="85" color="#ED6647"/>
                                        <dvtm:threshold id="tthcomp2" maxValue="93" color="#FAD55C"/>
                                        <dvtm:threshold id="tthcomp3" maxValue="107" color="#68C182"/>
                                        <dvtm:threshold id="tthcomp4" maxValue="115" color="#FAD55C"/>
                                        <dvtm:threshold id="tthcomp5" maxValue="120" color="#ED6647"/>
                                        <dvtm:referenceLine id="rl21" lineStyle="solid" value="100" color="black"/>
                                        <dvtm:metricLabel id="ml2">
                                            <amx:convertNumber id="cn2" integerOnly="true"/>
                                        </dvtm:metricLabel>
                                    </dvtm:statusMeterGauge>
                                </amx:cellFormat>
                            </amx:rowLayout>
                            <amx:rowLayout id="compftrs">
                                <amx:cellFormat id="cftenftr" width="50%" halign="center" height="40px" valign="top">
                                    <amx:outputText value="Years" id="ottenftr1" styleClass="profilesummary_subtext"/>
                                </amx:cellFormat>
                                <amx:cellFormat id="cfcompftr" width="50%" halign="center" valign="top">
                                    <amx:outputText value="#{Salary.inputValue}" id="otsalftr1"
                                                    styleClass="profilesummary_subtext">
                                        <amx:convertNumber id="cn1" type="currency" maxFractionDigits="0"
                                                           integerOnly="true" groupingUsed="true" currencySymbol="$"/>
                                    </amx:outputText>
                                </amx:cellFormat>
                            </amx:rowLayout>
                        </amx:tableLayout>
                    </amx:facet>
                </amx:panelStretchLayout>
            </amx:filmStripItem>
            <amx:filmStripItem id="summaryp3" rendered="#{deviceScope.hardware.screen.diagonalSize le 6}">
                <amx:panelStretchLayout id="psl7" scrollPolicy="scroll">
                    <amx:facet name="top">
                        <amx:tableLayout id="tl4" width="100%">
                            <amx:rowLayout id="rl13">
                                <amx:cellFormat id="cf26" width="100%" halign="center">
