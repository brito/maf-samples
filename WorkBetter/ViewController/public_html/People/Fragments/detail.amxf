                        </amx:iterator>
                    </amx:facet>
                </amx:panelStretchLayout>
            </amx:filmStripItem>
            <amx:filmStripItem id="summaryp4" rendered="#{deviceScope.hardware.screen.diagonalSize le 6}">
                <amx:panelStretchLayout id="psl6" scrollPolicy="scroll">
                    <amx:facet name="top">
                        <amx:tableLayout id="tl9" width="100%">
