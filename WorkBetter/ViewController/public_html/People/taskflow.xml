<?xml version="1.0" encoding="US-ASCII" ?>
<adfc-mobile-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.0">
  <task-flow-definition id="taskflow">
    <default-activity>people</default-activity>
    <task-flow-call id="people">
      <task-flow-reference>
        <document>/People/people.xml</document>
        <id>people</id>
      </task-flow-reference>
      <input-parameter id="__1">
        <name>masterView</name>
        <value>list</value>
      </input-parameter>
      <input-parameter id="__2">
        <name>summaryView</name>
        <value>summary</value>
      </input-parameter>
    </task-flow-call>
  </task-flow-definition>
</adfc-mobile-config>
