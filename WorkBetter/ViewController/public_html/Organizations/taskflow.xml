<?xml version="1.0" encoding="US-ASCII" ?>
<adfc-mobile-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.0">
  <task-flow-definition id="taskflow">
    <default-activity>organizations</default-activity>
    <task-flow-call id="organizations">
      <task-flow-reference>
        <document>/Organizations/organizations.xml</document>
        <id>organizations</id>
      </task-flow-reference>
      <input-parameter id="__1">
        <name>masterView</name>
        <value><?audit suppress oracle.ide.xml.brokenref-warning?><?audit suppress oracle.adfdt.controller.adfc.source.audit.ELUnknownToken?>#{deviceScope.hardware.screen.diagonalSize gt 6 ? 'cardt' : 'cardp'}</value>
      </input-parameter>
      <input-parameter id="__2">
        <name>summaryView</name>
        <value>summary</value>
      </input-parameter>
      <input-parameter id="__3">
        <name>showMeetings</name>
        <value>true</value>
      </input-parameter>
      <input-parameter id="__4">
        <name>showEvents</name>
        <value>true</value>
      </input-parameter>
      <input-parameter id="__5">
        <name>showTraining</name>
        <value>true</value>
      </input-parameter>
      <input-parameter id="__6">
        <name>showTimeOff</name>
        <value>true</value>
      </input-parameter>
    </task-flow-call>
  </task-flow-definition>
</adfc-mobile-config>
